{
  "name": "golja-flowdockreport",
  "version": "0.2.0",
  "author": "Dejan Golja",
  "summary": "A Puppet report processor that announces report results to Flowdock",
  "license": "Apache-2.0",
  "source": "https://github.com/n1tr0g/puppet-flowdockreport",
  "project_page": "https://github.com/n1tr0g/puppet-flowdockreport",
  "issues_url": "https://github.com/n1tr0g/puppet-flowdockreport/issues",
  "tags": [
    "flowdock",
    "puppet_reports"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "6",
        "7",
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 1.0.0"}
  ]
}
